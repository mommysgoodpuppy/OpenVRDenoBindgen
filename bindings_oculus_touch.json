{
  "action_manifest_version": 0,
  "alias_info": {},
  "bindings": {
    "/actions/main": {
      "poses": [
        {
          "output": "/actions/main/in/HandPoseLeft",
          "path": "/user/hand/left/pose/raw"
        },
        {
          "output": "/actions/main/in/HandPoseRight",
          "path": "/user/hand/right/pose/raw"
        }
      ],
      "sources": [
        {
          "inputs": {
            "click": {
              "output": "/actions/main/in/TriggerLeft"
            }
          },
          "mode": "button",
          "path": "/user/hand/left/input/trigger"
        },
        {
          "inputs": {
            "click": {
              "output": "/actions/main/in/TriggerRight"
            }
          },
          "mode": "button",
          "path": "/user/hand/right/input/trigger"
        }
      ]
    }
  },
  "category": "steamvr_input",
  "controller_type": "oculus_touch",
  "description": "Binding for Oculus Touch Controller",
  "name": "Oculus Touch Controller",
  "options": {},
  "simulated_actions": []
}